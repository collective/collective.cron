<html xmlns="http://www.w3.org/1999/xhtml"
  xml:lang="en" lang="en"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  metal:use-macro="context/main_template/macros/master"
  i18n:domain="collective.cron">
  <body>
    <metal:main fill-slot="main">
      <tal:main-macro metal:define-macro="main"
        tal:define="toLocalizedTime nocall:context/@@plone/toLocalizedTime;">
        <h1>Available backends</h1>
        <ul class="icons-on">
          <tal:for tal:repeat="i view/getBackends">
            <li tal:define="icon python:view.getBackendIcon(i)">
            <a tal:attributes="class string:contenttype-${i/portal_type/lower};
              href i/getURL">
              <img tal:attributes="src string:$icon"/>
              <span tal:content="string:${i/Title}">TITLE</span>
            </a>
            </li>
          </tal:for>
        </ul>
      </tal:main-macro>
    </metal:main>
  </body>
</html>
